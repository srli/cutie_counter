{
  "name": "cutie-counter",
  "productName": "cutie-counter",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "./dist/index.js",
  "repository": "https://github.com/srli/cutie_counter",
  "scripts": {
    "clean": "rm -rf dist node_modules out",
    "build": "tsc",
    "start": "npm run build && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint -c .eslintrc.yml ../cutie-counter/src --ext .ts",
    "lint:fix": "eslint -c .eslintrc.yml ../cutie-counter/src --ext .ts --fix"
  },
  "keywords": [],
  "author": {
    "name": "srli",
    "email": "li.sophie.43@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cutie_counter"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "@types/node": "12.12.6",
    "@types/quill": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "electron": "7.1.10",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "quill": "^1.3.7",
    "react": "^16.12.0",
    "typescript": "^3.7.5"
  }
}
